<div id="<%= dom_id course %>">
  <p>
    <strong>Event:</strong>
    <%= course.event_id %>
  </p>

  <p>
    <strong>Event name:</strong>
    <%= course.event_name %>
  </p>

  <p>
    <strong>Event category:</strong>
    <%= course.event_category %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= course.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= course.end_date %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= course.location %>
  </p>

  <p>
    <strong>Professor first name:</strong>
    <%= course.professor_first_name %>
  </p>

  <p>
    <strong>Professor last name:</strong>
    <%= course.professor_last_name %>
  </p>

  <p>
    <strong>Repetition frequency:</strong>
    <%= course.repetition_frequency %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= course.url %>
  </p>

</div>
